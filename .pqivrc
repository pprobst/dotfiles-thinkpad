[options]
transparent-background=0
fade=1
hide-info-box=1

[keybindings]
k  { goto_file_relative(-1) }
j  { goto_file_relative(1) }
J  { goto_file_relative(10) }
K  { goto_file_relative(-10) }

<Control>p  { goto_directory_relative(-1) }
<Control>n  { goto_directory_relative(1) }

d  { quit() }
r  { reload() }
f  { toggle_fullscreen() }

l  { set_scale_level_relative(1.1) }
h  { set_scale_level_relative(0.9) }
t  { toggle_scale_mode(0) }

/  { flip_horizontally() }
?  { flip_vertically() }

i  { toggle_info_box() }
g  { jump_dialog() }

,  { rotate_right() }
.  { rotate_left() }
